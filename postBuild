jupyter nbextension enable --py --sys-prefix xm_magic
jupyter nbextension enable --py --sys-prefix xm_magic/sigplotjs
jupyter nbextension enable --py --sys-prefix xm_magic/sigplotjs/sigplot_ext
jupyter nbextension enable --py --sys-prefix xm_magic/sigplotjs/sigplot_debug
jupyter nbextension enable --py --sys-prefix xm_magic/sigplotjs/sigplot_ext/widget_sigplot
jupyter nbextension enable --py --sys-prefix xm_magic/sigplotjs/sigplot_ext/widget_test
jupyter trust .
